{
	"root": true,
	"env": {
		"es2022": true
	},
	"extends": ["@w0s/eslint-config"],
	"ignorePatterns": ["dist/*.js"],
	"overrides": [
		{
			"files": ["src/*.js"],
			"env": {
				"browser": true
			},
			"parserOptions": {
				"sourceType": "script"
			},
			"rules": {
				"no-alert": "off"
			}
		}
	]
}
